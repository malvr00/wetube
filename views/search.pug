extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for: #{searchingBy}
    .search__videos
            if Videos.length === 0
                h5 No Videos Found
            each video in Videos
                +videoBlock({
                    title: video.title,
                    views: video.views,
                    videoFile: video.videoFile,
                    id:video.id
            })